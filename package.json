{
  "type": "module",
  "scripts": {
    "build": "rollup --config",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "dev": "tsx --require dotenv/config --env-file .env.development ./src/index.ts dotenv_config_path=./.env",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "prod": "tsx --require dotenv/config --env-file .env.production ./src/index.ts dotenv_config_path=./.env"
  },
  "dependencies": {
    "@date-fns/utc": "2.1.0",
    "axios": "1.8.4",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.41.0",
    "inversify": "7.7.0",
    "postgres": "3.4.5",
    "reflect-metadata": "0.2.2",
    "zod": "4.0.0-beta.20250411T195319"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/node": "22.18.3",
    "dotenv": "17.2.2",
    "drizzle-kit": "0.31.4",
    "eslint": "9.24.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.2",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.19",
    "rollup": "4.39.0",
    "tslib": "2.8.1",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.0"
  },
  "engines": {
    "node": "22.19.0"
  }
}
