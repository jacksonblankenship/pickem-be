{
  "type": "module",
  "scripts": {
    "build": "rollup --config",
    "cli": "tsx --require dotenv/config --env-file .env.development ./src/index.ts",
    "cli:prod": "tsx --require dotenv/config --env-file .env.production ./src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@date-fns/utc": "2.1.1",
    "axios": "1.8.4",
    "commander": "14.0.1",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.41.0",
    "inversify": "7.7.0",
    "pino": "9.9.5",
    "pino-pretty": "13.1.1",
    "postgres": "3.4.7",
    "reflect-metadata": "0.2.2",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/node": "22.18.4",
    "dotenv": "17.2.2",
    "drizzle-kit": "0.31.4",
    "eslint": "9.35.0",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "rollup": "4.50.2",
    "tslib": "2.8.1",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0"
  },
  "engines": {
    "node": "22.19.0"
  }
}
